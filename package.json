{
    "name": "auth0-singpass-connection-extension",
    "version": "1.8.0",
    "description": "Singpass token wrapper and other endpoints to support private key jwt / client assertion",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean && npm run extension:build",
        "clean": "rimraf dist",
        "extension:build": "a0-ext build:server ./index.js ./dist && cp ./dist/auth0-singpass-connection-extension.extension.$npm_package_version.js ./build/bundle.js"
    },
    "keywords": [
        "auth0",
        "client-assertion",
        "singpass",
        "private_key_jwt"
    ],
    "author": "pushpabrol",
    "license": "ISC",
    "dependencies": {
        "auth0": "^3.0.1",
        "axios": "^0.27.2",
        "cors": "^2.8.1",
        "dotenv": "^16.0.1",
        "express": "^4.17.1",
        "jose": "^4.14.6",
        "jsonwebtoken": "^9.0.0",
        "node-jose": "^2.2.0",
        "qs": "^6.5.2",
        "superagent": "^5.3.1",
        "uuid": "^8.3.1",
        "webtask-tools": "^3.4.1"
    },
    "auth0-extension": {
        "useBabel": false,
        "useHashName": false,
        "externals": [
            "express@4.17.1",
            "webtask-tools",
            "node-jose@2.0.0",
            "jose@4.10.0",
            "axios@0.27.2",
            "uuid@8.3.1",
            "qs@6.5.2",
            "dotenv@16.0.1",
            "superagent@5.3.1",
            "auth0@3.0.1",
            "jsonwebtoken@9.0.0",
            "cors@2.8.1"
        ],
        "excluded": [
            "safe-eval",
            "express-conditional-middleware",
            "pino"
        ]
    },
    "devDependencies": {
        "auth0-extensions-cli": "^4.0.4"
    }
}
